{
  "vim.camelCaseMotion.enable": true,
  "vim.foldfix": true,
  "window.zoomLevel": 0,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["w"],
      "after": ["<leader>", "w"]
    },
    {
      "before": ["e"],
      "after": ["<leader>", "e"]
    },
    {
      "before": ["b"],
      "after": ["<leader>", "b"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    }
  ],
  "vim.visualstar": true,
  "vim.hlsearch": true
}
